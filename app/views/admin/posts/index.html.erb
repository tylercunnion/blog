<table>
	<thead>
		<td>Title</td>
		<td>Author</td>
		<td>Published?</td>
		<td>Created</td>
		<td>Edit</td>
		<td>View</td>
	</thead>
	<tbody>
<% @posts.each do |post| -%>
		<tr>
			<td><%= post.title %></td>
			<td><%= post.author.name %></td>
			<td><%= post.created_at %></td>
			<td><%= link_to("Edit", edit_admin_post_url(post)) %></td>
			<td><%= link_to("View", post) %></td>
		</tr>
<% end -%>
	</tbody>
</table>